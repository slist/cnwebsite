node {
    stage('Download source code') {
        // Get some code from a GitHub repository
        git 'https://github.com/slist/cnwebsite.git'
    }
    stage('Build') {
        sh 'docker build -t cnwebsite .'
    }
    stage('Run') {
       sh 'docker run --rm -d -p 80:80 --name web cnwebsite'
    }
}
      
